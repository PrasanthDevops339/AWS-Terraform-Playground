{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ExampleRoleAccessReplica",
      "Effect": "Allow",
      "Principal": { "AWS": "${example_role_arn}" },
      "Action": [
        "kms:Encrypt",
        "kms:Decrypt",
        "kms:ReEncrypt*",
        "kms:GenerateDataKey*",
        "kms:DescribeKey"
      ],
      "Resource": "*"
    }
  ]
}
